        <stringProp name="script">import org.apache.jmeter.util.JMeterUtils; 
import org.apache.jmeter.samplers.SampleResult; 
import java.net.*;
import java.io.*;

// I am sure there is a more efficient way to create that URL - e.g: using StringBuffer
String baseUrl = &quot;http://localhost:4000/genericmonitor?&quot;;
String splitName = prev.getSampleLabel().replaceAll(&quot; &quot;, &quot;%20&quot;);
String timeValue = splitName + &quot;:Time=&quot; + String.valueOf(prev.getTime());
String byteValue = &quot;Bytes=&quot; + String.valueOf(prev.getBytes());
String connectValue = &quot;Connect%20Time=&quot; + String.valueOf(prev.getConnectTime());
String errorValue = &quot;Error%20Count=&quot; + String.valueOf(prev.getErrorCount());
String latencyValue = &quot;Latency=&quot; + String.valueOf(prev.getLatency());
String countValue = &quot;Count=&quot; + String.valueOf(prev.getSampleCount());
String fullUrl = baseUrl + timeValue + &quot;,&quot; + byteValue + &quot;,&quot; + connectValue + &quot;,&quot; + errorValue + &quot;,&quot; + latencyValue + &quot;,&quot; + countValue;

URL u = new URL(fullUrl);
InputStream iStream = u.openConnection().getInputStream();
String nextLine = new BufferedReader(new InputStreamReader(iStream)).readLine();
iStream.close();</stringProp>
